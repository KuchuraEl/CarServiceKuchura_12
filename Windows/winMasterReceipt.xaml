<Window x:Class="CarServiceKuchura_12.Windows.winMasterReceipt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarServiceKuchura_12.Windows"
        mc:Ignorable="d"
        Title="Окно мастера-приемщика" MinHeight="350" MinWidth="700" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button x:Name="btnUsers" Content="Список сотрудников" Click="btnUsers_Click"/>
            <Button x:Name="btnOrders" Content="Список заказов" Click="btnOrders_Click"/>
            <Button x:Name="btnSmenu" Content="Список смен" Click="btnSmenu_Click"/>

            <Button x:Name="btnBack" Content="НАЗАД" Click="btnBack_Click"/>
        </StackPanel>
        
        
    </Grid>
</Window>
